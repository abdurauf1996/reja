              </div>
              <div class="one_language">
                <div class="tepa">
                  <div class="chap">English</div>
                  <div class="ong">80%</div>
                </div>
                <div class="past">
                  <div class="tashqari">
                    <div class="ichkari sakson"></div>
                  </div>
                </div>
              </div>
              <div class="one_language">
                <div class="tepa">
                  <div class="chap">Spanish</div>
                  <div class="ong">60%</div>
                </div>
                <div class="past">
                  <div class="tashqari">
                    <div class="ichkari oltmish"></div>
                  </div>
                </div>
              </div>
            </div>
          </div>
          <div class="marginer"></div>
          <div class="left_4"></div>
          <div class="marginer"></div>
          <div class="left_5"></div>
          <div class="marginer"></div>
        </div>
        <div class="midd_frame"></div>
        <div class="right_frame"></div>
